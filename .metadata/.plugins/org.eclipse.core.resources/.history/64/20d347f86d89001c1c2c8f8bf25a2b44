package ws0209;

import java.util.Scanner;

public class No3_BOJ2578_빙고 {

	static int[][] mat = new int[5][5];
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		for(int r=0; r<5; r++) {
			for(int c=0; c<5; c++) {
				mat[r][c] = sc.nextInt();
			}
		}
		
		for(int i=0; i<25; i++) {
			int num = sc.nextInt();
			
			for(int r=0; r<5; r++) {
				for(int c=0; c<5; c++) {
					if(num == mat[r][c]) {
						mat[r][c] = 100;
						boolean ch = check();
					}
				}
			}
			System.out.println(i+1);
		}
	}
	public static boolean check() {
		for(int r=0; r<5; r++) {
			for(int c=0; c<5; c++) {
				if(mat[r][c] !=100) {
					break;
				}
				//여기까지 오면 row 빙고
				return true;
			}
		}
		for(int c=0; c<5; c++) {
			for(int r=0; r<5; r++) {
				if(mat[r][c] !=100) {
					break;
				}
				//여기까지 오면 col 빙고
				return true;
			}
		}
		return false;
	}
	
}
