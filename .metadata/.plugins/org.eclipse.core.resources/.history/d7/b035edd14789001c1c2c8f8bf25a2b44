package ws0209;

import java.util.Arrays;
import java.util.Scanner;

public class BOJ16926_배열돌리기 {
	static int[] dr = {1,0,-1,0};//하,우,상,좌
	static int[] dc = {0,1,0,-1};
	static int N, M, R; //열,행,회전수
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		N = sc.nextInt();
		M = sc.nextInt();
		R = sc.nextInt();
		
		int[][] arr = new int[N][M];
		
		for(int i=0; i<N; i++) {
			for(int j=0; j<M; j++) {
				arr[i][j] = sc.nextInt();
			}
		}//입력 끝
		
		for(int r=0; r<R; r++) {
			for(int i=0; i<Math.min(M,N); i++) {
				int row = i;
				int col = i;
				int idx = 0;
				int value = arr[row][col];
				
				while(idx < 4) {
					int nr = row + dr[idx];
					int nc = col + dc[idx];
					
					if(nc>=i && nr>=i && nr<N-i && nc<M-i) {
						arr[row][col] = arr[nr][nc];
						row = nr;
						col = nc;
					}
					else idx++;
					
				}
				arr[i+1][i] = value;
			}
		}
		
		for(int i=0; i<N; i++) {
			for(int j=0; j<M; j++) {
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
		
	}

}
