package ws0209;

import java.util.Scanner;

public class No5_BOJ2564_경비원 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int W = sc.nextInt();
		int H = sc.nextInt();
		
		int[][] mat = new int[H+1][W+1];
		int R = sc.nextInt();
		for(int i=0; i<R; i++) {
			int dir = sc.nextInt();
			int dis = sc.nextInt();
			if(dir == 1) {
				mat[0][dis] = i+1;
			}
			if(dir == 2) {
				mat[H][dis] = i+1;
			}
			if(dir == 3) {
				mat[dis][1] = i+1;
			}
			if(dir == 4) {
				mat[dis][W] = i+1;
			}
		}
		
		///입력끝
		
		for(int r=0; r<W+1; r++) {
			for(int c=0; c<H+1; c++) {
				System.out.print(mat[r][c]);
			}
			System.out.println();
		}
		
	}
}
