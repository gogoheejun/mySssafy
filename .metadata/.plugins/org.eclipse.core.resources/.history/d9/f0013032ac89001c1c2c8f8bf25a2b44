package ws0209;

import java.io.IOException;

import java.util.Scanner;

public class 연습장 {

	public static void main(String[] args) throws IOException {

		Scanner sc = new Scanner(System.in);
		for (int t = 1; t <= 10; t++) {
			int tc = sc.nextInt();
			int find = 0;
			int[][] a = new int[100][100];

			for (int i = 0; i < 100; i++) {
				for (int j = 0; j < 100; j++) {
					a[i][j] = sc.nextInt();
				}
			}
		}
	}
}