package ws0209;

import java.util.Scanner;

import javax.sound.midi.Soundbank;

public class No3_BOJ2578_빙고 {

	static int[][] mat = new int[5][5];
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		for(int r=0; r<5; r++) {
			for(int c=0; c<5; c++) {
				mat[r][c] = sc.nextInt();
			}
		}//입력완료
		
		for(int i=0; i<25; i++) {
			int num = sc.nextInt();
			boolean ch = false;
			System.out.println("카운트: "+i);
			for(int r=0; r<5; r++) {
				for(int c=0; c<5; c++) {
					if(num == mat[r][c]) {
						mat[r][c] = 100;
						ch = check();
					}
				}
			}
			if(ch==true) {
				System.out.println(i+1);
				return;
			}
		}
	}
	public static boolean check() {
		for(int r=0; r<5; r++) {
			int ok=0;
			for(int c=0; c<5; c++) {
				if(mat[r][c] ==100) {
					ok+=1;
					
//					System.out.println("왔니");
//					for(int t=0; t<5; t++) {
//						System.out.print(mat[r][t] +" ");
//					}
//					System.out.println();
				}
			}
			if(ok == 5) {
//				System.out.println("트루!!!!!");
				return true;
			}
			
		}
		for(int c=0; c<5; c++) {
			int ok=0;
			for(int r=0; r<5; r++) {
				
				if(mat[r][c] ==100) {
					ok+=1;
					
					System.out.println("왔니2");
					for(int t=0; t<5; t++) {
						System.out.print(mat[t][c] +" ");
					}
					System.out.println();
				}				
			}
			if(ok == 5) {
				System.out.println("트루!!!!!2");
				for(int x=0; x<5; x++) {
					for(int y=0; y<5; y++) {
						System.out.println(mat[x][y]);
					}
				}//입력완료
				return true;
			}
		}
		return false;
	}
	
}
